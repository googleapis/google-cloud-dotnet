              "methods": [
                "GetIamPolicy",
                "GetIamPolicyAsync"
              ]
            },
            "SetIamPolicy": {
              "methods": [
                "SetIamPolicy",
                "SetIamPolicyAsync"
              ]
            },
            "TestIamPermissions": {
              "methods": [
                "TestIamPermissions",
                "TestIamPermissionsAsync"
              ]
            }
          }
        }
      }
    }
  }
}
