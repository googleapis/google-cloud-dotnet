{
  "schema": "1.0",
  "comment": "This file maps proto services/RPCs to the corresponding library clients/methods",
  "language": "csharp",
  "protoPackage": "google.maps.weather.v1",
  "libraryPackage": "Google.Geo.Weather.V1",
  "services": {
    "Weather": {
      "clients": {
        "grpc": {
          "libraryClient": "WeatherClient",
          "rpcs": {
            "LookupCurrentConditions": {
              "methods": [
                "LookupCurrentConditions",
                "LookupCurrentConditionsAsync"
              ]
            },
            "LookupForecastDays": {
              "methods": [
                "LookupForecastDays",
                "LookupForecastDaysAsync"
              ]
            },
            "LookupForecastHours": {
              "methods": [
                "LookupForecastHours",
                "LookupForecastHoursAsync"
              ]
            },
            "LookupHistoryHours": {
              "methods": [
                "LookupHistoryHours",
                "LookupHistoryHoursAsync"
              ]
            },
            "LookupPublicAlerts": {
              "methods": [
                "LookupPublicAlerts",
                "LookupPublicAlertsAsync"
              ]
            }
          }
        }
      }
    }
  }
}
